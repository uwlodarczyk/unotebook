
      <header>
        <div class="logo"><img src="" alt=""></div>
      </header>

      <div class="main">

        <div class="title">
          <h1>software testing<span>zone</span></h1>                  
        </div>
      <div class="post-content">

        <% @posts.each do |post| %>
          <div class="single-post">
            <h4><%= post.title %></h4>
            <div class="post-holder"><%= post.body %></div>
            <div class="edit-links">
              <%= link_to 'Show', post %>
              <%= link_to 'Edit', edit_post_path(post) %>  
              <%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %>   
            </div>
          </div>
        <% end %>     

      <div class="edit-links create"><%= link_to 'New Post', new_post_path %></div>
      
      </div><!-- /post-content -->







      </div><!-- /main -->


      <footer><div class="copy">&copy;<h6>uNOTEBOOK</h6>2013</div></footer>






























